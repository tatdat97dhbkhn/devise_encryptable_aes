# frozen_string_literal: true

source 'https://rubygems.org'
git_source(:github) { |repo| "https://github.com/#{repo}.git" }

# Use alternative (and even your own!) encryptors with Devise. [https://github.com/heartcombo/devise-encryptable]
gem 'devise-encryptable'

# An AES encrypt/decrypt gem built ontop of OpenSSL [https://rubygems.org/gems/aes]
gem 'aes', '~> 0.5.1'
